{
  "manifest_version": 3,
  "name": "My first Extension",
  "version": "0.3",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://gemini.google.com/*", "*://chat.openai.com/*"], 
      "js": ["./content.js"]
    }
  ],
  "action": {
    "default_popup": "index.html"
  }
}
