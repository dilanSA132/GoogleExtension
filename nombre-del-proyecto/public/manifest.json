{
  "manifest_version": 3,
  "name": "IA Generate",
  "version": "0.3",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": ["*://gemini.google.com/*", "*://chat.openai.com/*"], 
      "js": ["./content.js"]
    }
  ],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "Icon.png",  
      "48": "Icon.png",  
      "128": "Icon.png"  
    }
  }
}
